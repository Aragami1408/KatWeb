{
  "cachingTimeout": 4,
  "streamTimeout": 10,
  "hsts": false,
  "letsencrypt": {
    "enabled": false,
    "domains": ["example.com"]
  },
  "proxy": [
    {
      "location": "proxy2",
      "host": "http://example.com"
    },
    {
      "location": "proxy",
      "host": "http://localhost:8080/"
    }
  ],
  "redir": [
    {
	"location": "localhost/redirect",
	"dest": "http://example.com"
    }
  ],
  "advanced": {
    "protect": true,
    "logging": true,
    "httpPort": 80,
    "sslPort": 443
  }
}
