{
  "cachingTimeout": 4,
  "streamTimeout": 10,
  "hsts": false,
  "protect": true,
  "performance": {
    "logging": true,
    "threads": 0,
    "gcx": 2.5,
    "gzipx": 2
  },
  "proxy": [
    {
      "location": "proxy2",
      "host": "http://example.com"
    },
    {
      "location": "proxy",
      "host": "http://localhost:8080/"
    }
  ],
  "redir": [
    {
	"location": "localhost/redirect",
	"dest": "http://example.com"
    }
  ],
  "name": "KatWeb",
  "httpPort": 80,
  "sslPort": 443
}
